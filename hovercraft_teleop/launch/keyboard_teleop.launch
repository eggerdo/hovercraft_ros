<launch>
  <!-- hovercraft_teleop_key already has its own built in velocity smoother -->
  <node pkg="hovercraft_teleop" type="hovercraft_teleop_key" name="hovercraft_teleop_keyboard"  output="screen">
    <param name="scale_linear" value="0.15" type="double"/>
    <param name="scale_angular" value="1.0" type="double"/>
    <!-- <remap from="/cmd_vel" to="cmd_vel_mux/input/teleop"/> -->
  </node>
</launch>
